  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console) core 2.2",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Cli/bin/Debug/netcoreapp2.2/Cli.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/Cli",
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": ".NET Core Launch (console) standard 2.0",
      "type": "clr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Cli/bin/Debug/netstandard2.0/Cli.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/Cli",
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
